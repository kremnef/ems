<?xml version="1.0"?>
<jx:template xmlns:jx="http://apache.org/cocoon/templates/jx/1.0">
    <jx:forEach select="#{language}" xmlns:jx="http://apache.org/cocoon/templates/jx/1.0">
            <language id="#{id}">
                <name>#{name}</name>
                <code>#{code}</code>
                <charset>#{charset}</charset>
            </language>
        </jx:forEach>
</jx:template>