<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="ru.strela.ems.security">
    <!--<union-subclass name="Customer" table="users">-->
    <class name="ru.strela.ems.security.model.Group" table="groups">
        <id name="groupId" column="id" type="int">
            <generator class="increment"/>
        </id>
        <property name="groupName" column="group_name" type="string" not-null="true" access="field"/>

    </class>
    <!--</union-subclass>-->
</hibernate-mapping>

